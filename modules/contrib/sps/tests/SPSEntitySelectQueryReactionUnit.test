  protected function getTestCondition($overrides = NULL) {
    $manager = new \Drupal\sps\Test\Manager();
    $controller = new \Drupal\sps\Test\TableOverrideStorageController(array(), $manager);
    if($overrides) {
      $controller ->set($overrides);
    }
    return $controller;
  }

